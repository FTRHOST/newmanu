---
import config from "@/config/config.json";
import { Image } from "astro:assets";
const { src } = Astro.props;
const {
  logo,
  logo_width,
  logo_height,
  logo_text,
  title,
}: {
  logo: string;
  logo_width: any;
  logo_height: any;
  logo_text: string;
  title: string;
} = config.site;
---

<a href="/" class="navbar-brand block flex items-center">
  <Image
    src={src ? src : logo}
    width={logo_width}
    height={logo_height}
    alt={title}
    priority={true}
    loading="eager"
    format="svg"
/>
      <span class="ml-2">MA NU 01 BANYUPUTIH</span>
</a>
