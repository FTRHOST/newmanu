---
import Base from "@/layouts/Base.astro";
import Banner from "@/layouts/components/Banner.astro";
import Cta from "@/layouts/partials/Cta.astro";
import type { CollectionEntry } from "astro:content";
import { getEntry } from "astro:content";

import Videop from "src/layouts/VideoProfile.astro"
import Promin from "src/layouts/components/Promin.astro"

const homepage = (await getEntry(
  "homepage",
  "-index"
)) as CollectionEntry<"homepage">;
const { key_features, service, testimonial } = homepage.data;
---

<Base image="/images/banner-img.webp" description="Interface Website Baru MA NU 01 Banyuputih">

  <Banner />
  <Videop />
  <Promin />

<Cta />

</Base>
